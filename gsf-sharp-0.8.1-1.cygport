inherit mono

DESCRIPTION="Mono libgsf-1 bindings"
HOMEPAGE="http://www.mono-project.com/"
SRC_URI="http://primates.ximian.com/~joe/${P}.tar.gz"

src_install() {
	cd ${B}
	cyginstall

	# Mono doesn't like these stripped
	chmod -w ${D}/usr/bin/*glue*.dll

	sed -i -e 's#shouldnotlink=no#shouldnotlink=yes#' ${D}/usr/lib/lib*.la
}
